@InProceedings{mcdermott03:_reason,
  author =	 MCDERMOTT,
  title =	 {Reasoning about autonomous processes in an
                  estimated-regression planner},
  crossref =	 {ICAPS03},
  pages =	 {143--152},
  annote =	 {Describes an extension to Drew's previous
                  estimated-regression planner that's able to handle
                  piecewise linear change from autonomous
                  processes. He shows how use of autonomous processes
                  can be fit into his heuristic graph approach. The
                  results are interesting and expressive, but not
                  particularly efficient. Another issue he deals with
                  is more expressive objective functions than simple
                  step costs.}
}

