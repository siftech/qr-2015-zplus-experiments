@techreport{ garvey93survey,
  author =	 "Alan Garvey and Victor Lesser",
  title =	 "A Survey of Research in Deliberative Real-Time
                  Artificial Intelligence",
  number =	 "UM-CS-1993-084",
  institution =	 "Department of Computer Science, University of
                  Massachusetts",
  annote =	 {An invaluable survey of bounded
                  rationality/deliberation scheduling/meta-reasoning
                  from the standpoint of design-to-time, instead of
                  the anytime approaches of Dean and Boddy, Horvitz,
                  and Russell (although it touches on their
                  work). Discusses what they call
                  \emph{approximation-based} approaches to real-time
                  AI. Distinguishes between use of (interruptible)
                  anytime algorithms and ``multiple and approximate
                  methods.'' The latter is close to what SA-CIRCA does
                  (multiple formulations of the control problem are
                  effectively a library of different algorithms). But
                  CIRCA approach differs because the executive is not
                  deliberative. Points out that Garvey and Lesser
                  looked into q. of predictability of performance
                  profiles. Cites Etzioni '91 on maximum utility
                  (greedy) heuristic.},
  month =	 nov,
  year =	 "1993"
}

