@INCOLLECTION{Pearl:Jcond,
	AUTHOR = PEARL,
	TITLE = {Jeffrey's rule, passage of experience and 
		 neo-{B}ayesianism},
	BOOKTITLE = {Knowledge Representation and Defeasible Reasoning},
	PUBLISHER = KLUWER,
	YEAR = {1990},
	EDITOR = KYBURG# {and Ronald P. Loui and Greg N. Carlson},
	PAGES = {245--265},
	ANNOTE = {Discusses the relationship between Jeffrey
		  conditioning and the Bayesian method of virtual
		  conditionalization.  The former consists of taking 
		  information about the posterior probability of some
		  propositions and conditioning other propositions on them.
		  Pearl argues that this doesn't correctly take into account
		  causality and dependence and argues that J-conditioning
		  can best be achieved through virtual evidence on a 
		  graphical model of the distribution and its dependencies.
		  He gives examples where observations give information
		  about propositions and also {\it create} dependencies,
		  hence leading J-conditioning to absurd conclusions.
		  Pearl also dismisses minimum divergence as a way of 
		  choosing the distribution posterior to J-conditioning.}
}

