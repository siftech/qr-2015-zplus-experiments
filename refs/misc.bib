}



@Article{CuryKroghNiinomi:1998,
  author =	 {Jos\'e E. R. Cury and } # KROGH # { and Toshihiko
                  Niinomi},
  title =	 {Synthesis of Supervisory Controllers for Hybrid
                  Systems Based on Approximating Automata},
  journal =	 {{IEEE} Transactions on Automatic Control},
  year =	 {1998},
  OPTkey =	 {},
  OPTvolume =	 {43},
  OPTnumber =	 {4},
  OPTpages =	 {564--568},
  OPTmonth =	 apr,
  OPTnote =	 {},
  OPTannote =	 {Describes a technique for synthesizing supervisory
                  controllers based on discrete approximations of the
                  kinds of systems described in other publications by
                  Krogh's group. These are hybrid systems formalized
                  in a bit more of a linear-algebra/continuous control
                  framework, rather than in the automata theoretic
                  style of Henzinger. The approach is based on
                  synthesizing a discrete state machine that
                  over-approximates the hybrid system, and then
                  generating a controller for that system. This is
                  interesting, but more limited than the Wong-Toi
                  approach, which is able to set parameters in the
                  process of synthesis: here all the threshold points
                  must be given. They present details of the
                  over-approximation, which preserves safety, and they
                  give a condition that's sufficient for producing
                  liveness, as well, making the over-approximation
                  acceptable. The approximation process is based on
                  building discrete systems based on longer and longer
                  strings of events and control actions, until the
                  discrete system either has an acceptable controller
                  or converges to the discrete system generated by the
                  events of the underlying hybrid system (at which
                  point, if there's no good controller, we'd know it
                  was hopeless).}
}

